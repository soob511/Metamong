<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mycompany.metamong.daoSub2.Sub2ColumnDao">

	<select id="selectColumnByDic" parameterType="String" resultType="Column">
		SELECT 
		    column_name colNm
		FROM 
		    user_tab_columns
		WHERE 
		    table_name = UPPER(#{tableName})
	</select>

</mapper>